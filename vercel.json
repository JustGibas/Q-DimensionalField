{
  "version": 2,
  "builds": [
    {
      "src": "backend/**/*.ts",
      "use": "@vercel/deno"
    },
    {
      "src": "api/**/*.ts",
      "use": "@vercel/deno"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/$1"
    }
  ],
  "functions": {
    "api/**/*.[jt]s": {
      "runtime": "vercel-deno@3.1.0"
    }
  }
}
