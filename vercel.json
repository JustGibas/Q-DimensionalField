/**
 * This file contains the configuration options for deploying the VoxelSpace project on Vercel.
 * It includes build and routing configurations.
 * Each configuration option is explained in detail to help students understand its purpose.
 */
{
  "builds": [
    // Specifies the build configuration
    { "src": "web/main.js", "use": "@vercel/node" }
  ],
  "routes": [
    // Specifies the routing configuration
    { "src": "/(.*)", "dest": "web/main.js" }
  ]
}
