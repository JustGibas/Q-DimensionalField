{
  "build": {
    "outputDir": "dist",
    "assetsDir": "assets",
    "minify": true,
    "sourcemap": false
  },
  "server": {
    "port": 3000,
    "open": true,
    "proxy": {
      "/api": {
        "target": "http://localhost:8000",
        "changeOrigin": true,
        "secure": false
      }
    }
  },
  "plugins": [
    "vite-plugin-react",
    "vite-plugin-wasm"
  ]
}
